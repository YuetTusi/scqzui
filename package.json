{
	"name": "scqzui",
	"version": "2.0.0",
	"main": "dist/main.js",
	"repository": "https://github.com/YuetTusi/scqzui.git",
	"author": "cuiyue <cuiyue.msn@hotmail.com>",
	"license": "MIT",
	"description": "FetchUI for market version",
	"scripts": {
		"prebuild": "rimraf dist/**/*",
		"build": "webpack --config webpack.dev.js",
		"build:prod": "webpack --config webpack.prod.js",
		"prego": "tsc",
		"serve": "webpack serve --config webpack.dev.js",
		"go": "electron . --no-sandbox",
		"start": "cross-env NODE_ENV=development concurrently \"yarn run build\" \"yarn run serve\" \"yarn run go\"",
		"prepack": "yarn run build:prod",
		"pack": "electron-builder --dir",
		"predist": "rimraf dist/**/* && tsc && yarn run build:prod",
		"dist": "electron-builder"
	},
	"build": {
		"appId": "4c9e34def3049532",
		"productName": "智能检测系统",
		"copyright": "Copyright © 2022 北京万盛华通科技有限公司",
		"directories": {
			"buildResources": "static"
		},
		"asar": true,
		"asarUnpack": [
			"dist/main.js",
			"dist/renderer/fonts/**/*",
			"dist/renderer/images/**/*",
			"dist/renderer/default.js",
			"dist/renderer/default.html",
			"dist/renderer/protocol.js",
			"dist/renderer/protocol.html"
		],
		"files": [
			"dist/**/*",
			"node_modules/**/*",
			"src/config/**/*",
			"src/renderer/**/*",
			"icon.png",
			"package.json"
		],
		"extraResources": [
			{
				"from": "./data/help",
				"to": "./help",
				"filter": [
					"**/*"
				]
			}
		],
		"win": {
			"target": [
				"nsis",
				"zip"
			],
			"icon": "icon.png",
			"publisherName": "yuet",
			"artifactName": "${productName}.${ext}",
			"requestedExecutionLevel": "requireAdministrator"
		},
		"nsis": {
			"oneClick": false,
			"allowToChangeInstallationDirectory": true,
			"installerIcon": "installer.ico",
			"perMachine": true
		},
		"linux": {
			"target": [
				{
					"target": "AppImage",
					"arch": "x64"
				},
				{
					"target": "deb",
					"arch": "x64"
				}
			]
		}
	},
	"devDependencies": {
		"@fortawesome/fontawesome-svg-core": "^6.1.1",
		"@fortawesome/free-brands-svg-icons": "^6.1.1",
		"@fortawesome/free-regular-svg-icons": "^6.1.1",
		"@fortawesome/free-solid-svg-icons": "^6.1.1",
		"@fortawesome/react-fontawesome": "^0.1.18",
		"@types/archiver": "^5.3.1",
		"@types/async": "^3.2.20",
		"@types/cors": "^2.8.12",
		"@types/electron": "^1.6.10",
		"@types/ftp": "^0.3.33",
		"@types/ini": "^1.3.31",
		"@types/jquery": "^3.5.14",
		"@types/js-yaml": "^4.0.5",
		"@types/lodash": "^4.14.178",
		"@types/nedb": "^1.8.12",
		"@types/node-notifier": "^8.0.2",
		"@types/qrcode": "^1.4.2",
		"@types/react": "^17.0.39",
		"@types/react-dom": "^17.0.11",
		"@types/redux-logger": "^3.0.9",
		"@types/styled-components": "^5.1.23",
		"@types/tinycolor2": "^1.4.3",
		"@types/uuid": "^8.3.4",
		"@ztree/ztree_v3": "^3.5.48",
		"antd-dayjs-webpack-plugin": "^1.0.6",
		"classnames": "^2.3.1",
		"concurrently": "^7.0.0",
		"cross-env": "^7.0.3",
		"css-loader": "^6.6.0",
		"dva": "^2.6.0-beta.22",
		"dva-immer": "^1.0.0",
		"echarts": "^5.3.2",
		"electron": "^26.3.0",
		"electron-builder": "^24.6.4",
		"ftp": "^0.3.10",
		"html-webpack-plugin": "^5.5.0",
		"iconv-lite": "^0.6.3",
		"jquery": "^3.6.0",
		"less": "^4.1.2",
		"less-loader": "^10.2.0",
		"qrcode": "^1.5.0",
		"redux-logger": "^3.0.6",
		"rimraf": "^3.0.2",
		"stickpackage": "^3.1.5",
		"style-loader": "^3.3.1",
		"styled-components": "5.1.1",
		"ts-loader": "^9.2.6",
		"typescript": "^4.5.5",
		"webpack": "^5.69.1",
		"webpack-cli": "^4.9.2",
		"webpack-dev-server": "^4.7.4",
		"yaml-loader": "^0.6.0"
	},
	"dependencies": {
		"antd": "^4.22.7",
		"archiver": "^5.3.0",
		"async": "^3.2.4",
		"check-disk-space": "^3.3.1",
		"cors": "^2.8.5",
		"cpy": "^8.1.2",
		"dayjs": "^1.10.7",
		"express": "^4.18.1",
		"ini": "^2.0.0",
		"js-yaml": "^4.1.0",
		"lodash": "^4.17.21",
		"nedb": "^1.8.0",
		"node-notifier": "^10.0.1",
		"node-xlsx": "^0.21.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"sqlite3": "5.1.6",
		"tinycolor2": "^1.4.2",
		"uuid": "^8.3.2",
		"winston": "^3.6.0"
	}
}
